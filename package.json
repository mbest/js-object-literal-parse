{
  "name": "object-literal-parse",
  "version": "2.1.0",
  "description": "Splits an object literal string into a set of top-level key-value pairs",
  "main": "dist/object-literal-parse.js",
  "module": "dist/object-literal-parse.mjs",
  "types": "dist/object-literal-parse.d.ts",
  "scripts": {
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mbest/js-object-literal-parse.git"
  },
  "keywords": [
    "object-literal",
    "object",
    "parser"
  ],
  "author": "Michael Best <mbest@dasya.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mbest/js-object-literal-parse/issues"
  },
  "homepage": "https://github.com/mbest/js-object-literal-parse#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "jest": "^26.6.1",
    "lodash.camelcase": "^4.3.0",
    "rollup": "^2.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.28.0",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  }
}
